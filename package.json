{
  "name": "@rich-data/viewer",
  "description": "Interactive Data Viewer",
  "version": "2.15.6",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/himself65/data-viewer.git"
  },
  "author": "himself65 <himself65@outlook.com>",
  "bugs": "https://github.com/himself65/data-viewer/issues",
  "homepage": "https://github.com/himself65/data-viewer#readme",
  "keywords": [
    "react-18",
    "react",
    "react-json",
    "react-json-viewer",
    "array-viewer",
    "component",
    "interactive",
    "interactive-json",
    "json",
    "json-component",
    "json-display",
    "json-tree",
    "json-view",
    "json-viewer",
    "json-inspector",
    "json-tree",
    "tree",
    "tree-view",
    "treeview"
  ],
  "main": "./src/index.tsx",
  "module": "./src/index.mjs",
  "exports": {
    ".": "./src/index.tsx",
    "./package.json": "./package.json",
    "./theme/*.css": "./theme/*.css"
  },
  "publishConfig": {
    "access": "public",
    "types": "dist/index.d.ts",
    "jsdelivr": "dist/browser.js",
    "browser": {
      "vanilla": "dist/browser.js"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
      },
      "./theme/*": "./theme/*"
    }
  },
  "files": [
    "dist",
    "theme"
  ],
  "scripts": {
    "dev": "yarn workspace @rich-data/viewer-docs dev",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "lint": "npx eslint . --ext .ts,.tsx,.js,.jsx --cache --fix",
    "lint:ci": "npx eslint . --ext .ts,.tsx,.js,.jsx --cache --max-warnings 0",
    "build": "rollup -c rollup.config.ts --configPlugin swc3",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "test-storybook:coverage": "test-storybook --coverage"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "copy-to-clipboard": "^3.3.3",
    "zustand": "^4.3.6"
  },
  "lint-staged": {
    "!*.{ts,tsx,js,jsx}": "prettier --write --ignore-unknown",
    "*.{ts,tsx,js,jsx}": "npx eslint --cache --fix"
  },
  "peerDependencies": {
    "react": "^17 || ^18",
    "react-dom": "^17 || ^18"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-angular": "^17.4.4",
    "@rollup/plugin-alias": "^4.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@storybook/addon-coverage": "^0.0.8",
    "@storybook/addon-essentials": "^7.0.0-rc.8",
    "@storybook/addon-interactions": "^7.0.0-rc.8",
    "@storybook/addon-links": "^7.0.0-rc.8",
    "@storybook/blocks": "^7.0.0-rc.8",
    "@storybook/jest": "^0.0.11-next.1",
    "@storybook/react": "^7.0.0-rc.8",
    "@storybook/react-vite": "^7.0.0-rc.8",
    "@storybook/test-runner": "^0.10.0-next.11",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@swc/core": "^1.3.42",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^18.15.9",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@types/web": "^0.0.99",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.7",
    "@vitest/coverage-istanbul": "^0.29.7",
    "@vitest/ui": "^0.29.7",
    "eslint": "^8.36.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-unused-imports": "^2.0.0",
    "expect-type": "^0.15.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "nyc": "^15.1.0",
    "pinst": "^3.0.0",
    "prettier": "^2.8.7",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.20.2",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-swc3": "^0.8.1",
    "storybook": "^7.0.0-rc.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vitest": "^0.29.7"
  },
  "packageManager": "yarn@3.5.0",
  "workspaces": [
    "docs"
  ]
}
